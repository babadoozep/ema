  .bottom-components {
    text-align: center;
    -ms-text-justify: distribute-all-lines;
    text-justify: distribute-all-lines;
  }

  .left, .center, .right {
    width: 28vw;
    vertical-align: top;
    display: inline-block;
    /*border: solid green;*/
    box-shadow: -5px 5px 10px 3px gray;
  }

  .left {
    margin-left: 50px;
    float: left;
  }

  .center {

    padding-bottom: 10px;

  }

  .right {
    margin-right: 50px;
    float: right;
    text-align: left;
  }

  .today {
    font: 8.5vw 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: 100;
    color: darkorange;
    margin: 10px auto 45px auto;
    text-align: center;
    opacity: .5;
    width: 100%;
  }

  .center h4 {
    font: 12px 'Helvetica Neue', Helvetica, Arial, sans-serif;
    color: gray;
    text-align: right;
    opacity: .8;
    font-style: italic;
    margin-bottom: 8px;
    margin-right: 15px;
  }

  .buttons {
    height: 10px;
    margin-bottom: 15px;
  }

  .next {
    float: right;
    width: 70px;
    margin-right: 15px;
  }

  .toggle {
    float: right;
    width: 90px;
    margin-right: 5px;
  }

  .left ol {
    padding-left: 0;

  }

  .left li {
    font: 2vw 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: 100;
    text-align: center;
    list-style-type: none;
    padding: 15px;
    opacity: .5;
  }

  .date-list {
    margin: 0;
  }

  .left li:nth-child(odd) {
    background-color: lightgray;
  }

  .currentDay {
    border: solid darkorange 2px;
  }

  .guess {
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    text-align: left;
    height: 60px;
    font-size: 1.2vw;
    font-weight: 100;
    color: #8c8c8c;
    /*border: solid black 1px;*/
    padding-left: 0;

  }

  .item-list {
    /*border: solid red 1px;*/
    /*box-shadow: -5px 5px 10px 3px gray;*/
    margin: 0;
    padding-left: 0;
    list-style-type: none;

  }

  .final-image {
    width: 50vw;
    margin: 150px auto 0 auto;
  }

  .cube {
    width: 50vw;
  }

  .goodbye {
    text-align: center;
  }

  .goodbye p {
    text-align: center;
    font-family: 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-size: 1.1vw;
    font-weight: 200;
    color: #8c8c8c;
  }

  .refresh {

  }

  .delete-button {
    background: url('data:image/svg+xml;base64,'), url('data:image/svg+xml;base64,');
    background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(255, 255, 255, 0.5)), color-stop(100%, rgba(255, 255, 255, 0))), radial-gradient(#f2f2f2, #cccccc);
    background: -moz-linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), -moz-radial-gradient(#f2f2f2, #cccccc);
    background: -webkit-linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), -webkit-radial-gradient(#f2f2f2, #cccccc);
    background: linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), radial-gradient(#f2f2f2, #cccccc);
    -moz-border-radius: 50px;
    -webkit-border-radius: 50px;
    border-radius: 50px;
    -moz-box-shadow: inset 0 1px 1px rgba(191, 191, 191, 0.75), 0 2px 1px rgba(0, 0, 0, 0.25);
    -webkit-box-shadow: inset 0 1px 1px rgba(191, 191, 191, 0.75), 0 2px 1px rgba(0, 0, 0, 0.25);
    box-shadow: inset 0 1px 1px rgba(191, 191, 191, 0.75), 0 2px 1px rgba(0, 0, 0, 0.25);
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.5);
    border: 2px solid #e6e6e6;
    color: #b3b3b3;
    float: left;
    height: 20px;
    width: 20px;
    text-indent: -9999px;
    position: relative;
    text-decoration: none;
    display: inline-block;
    margin-right: 1.3vw;
    margin-left: 1.8vw;

  }

  .delete-button:after {
    content: "\2716";
    text-indent: 0;
    display: block;
    position: absolute;
    top: 0;
    right: 3px;
    font-size: 13px;

  }

  .delete-button:hover {
    color: #e6e6e6;
    background: url('data:image/svg+xml;base64,'), url('data:image/svg+xml;base64,');
    background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(255, 255, 255, 0.5)), color-stop(100%, rgba(255, 255, 255, 0))), radial-gradient(#b3b3b3, #8c8c8c);
    background: -moz-linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), -moz-radial-gradient(#b3b3b3, #8c8c8c);
    background: -webkit-linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), -webkit-radial-gradient(#b3b3b3, #8c8c8c);
    background: linear-gradient(rgba(255, 255, 255, 0.5), rgba(255, 255, 255, 0)), radial-gradient(#b3b3b3, #8c8c8c);
    text-shadow: 0 -1px 0 rgba(153, 153, 153, 0.5);
    cursor: pointer;
  }

  .delete-button:active {
    background: url('data:image/svg+xml;base64,'), url('data:image/svg+xml;base64,');
    background: -webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, rgba(255, 255, 255, 0.15)), color-stop(100%, rgba(255, 255, 255, 0))), radial-gradient(#8c8c8c, #808080);
    background: -moz-linear-gradient(rgba(255, 255, 255, 0.15), rgba(255, 255, 255, 0)), -moz-radial-gradient(#8c8c8c, #808080);
    background: -webkit-linear-gradient(rgba(255, 255, 255, 0.15), rgba(255, 255, 255, 0)), -webkit-radial-gradient(#8c8c8c, #808080);
    background: linear-gradient(rgba(255, 255, 255, 0.15), rgba(255, 255, 255, 0)), radial-gradient(#8c8c8c, #808080);
    text-shadow: 0 -1px 0 rgba(26, 26, 26, 0.5);
    cursor: pointer;
  }


@media screen and (max-width: 1100px) {

  .center {
    width: 450px;
    clear: both;
    display: block;
    margin: 0 auto 15px auto;

  }

  .left {
    display: none;

  }

  .right {
    display: block;
    width: 450px;
    float: none;
    margin: 10px auto 10px auto;

  }

  .guess {
    font-size: 20px;
  }

  .today {
    font: 40px 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: 100;
    margin: 40px auto 5px auto;
  }

}


@media screen and (max-width: 470px) {
  .center {
    width: 95vw;
    margin: 0 auto 15px auto;
  }

  .right {
    width: 95vw;
  }

  .today {
    font: 9vw 'Helvetica Neue', Helvetica, Arial, sans-serif;
    font-weight: 100;
    margin: 40px auto 5 auto;
  }
}